class Solution {
    public int removeDuplicates(int[] nums) {
        int len=nums.length;
        int expnums[]=new int[len];
        expnums[0]=nums[0];
        int temp=nums[0],count=0,j=1;
        for(int i=1;i<len;i++)
        {
            if(temp==nums[i])
                {
                    count++;
                }
                else
                {
                    expnums[j]=nums[i];
                    temp=nums[i];
                    j++;
                }
        }
        for(int i=0;i<len;i++)
        {
            if(i<len-count)
            {
                nums[i]=expnums[i];
            }
            else
            {
                nums[i]=-1;
            }
        }
        return len-count;
    }
}
